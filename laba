#include <stdio.h>

int main() {
    int a[] = {12, 12, 0, 23, 12, -211, 0, 12, 0, 0, 1328, 21, 12, 12, 356, 34, 0, 4, 0};
    int c = 0;
    
    for (int i = 0; i < sizeof(a) / sizeof(a[0]); i++) {
        if (a[i] == 0) {
            c++;
        }
    }
    
    printf("%d\n", c);
    
    return 0;
}
